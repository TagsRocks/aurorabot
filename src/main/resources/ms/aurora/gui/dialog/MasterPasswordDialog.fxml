<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Label?>
<fx:root type="javafx.scene.layout.AnchorPane" id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity"
         minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="320.0"
         xmlns:fx="http://javafx.com/fxml">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Password:" />
        <PasswordField fx:id="txtPassword" layoutX="75.0" layoutY="14.0" prefWidth="210.0" text=""/>
        <Label layoutX="14.0" layoutY="46.0" text="Repeat:" />
        <PasswordField fx:id="txtVerifyPassword" layoutX="75.0" layoutY="46.0" prefWidth="210.0" text=""/>
        <Label fx:id="warning" layoutX="14.0" layoutY="78.0" text="Incorrect Password!" visible="false"  />
        <Button fx:id="btnAuthenticate" layoutX="195.0" layoutY="78.0" mnemonicParsing="false"
                onAction="#authenticateAction" text="Authenticate"/>
    </children>
</fx:root>
